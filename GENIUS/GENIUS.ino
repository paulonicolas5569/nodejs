void setup() {
  // leds
    pinMode(2, OUTPUT);    //VERDE
    pinMode(3, OUTPUT); //VERMELHO
    pinMode(4, OUTPUT);  //AZUL
    pinMode(5, OUTPUT);  //AMARELO
}

void loop() {
  digitalWrite(2, HIGH);
  digitalWrite(3, HIGH);
  digitalWrite(4, HIGH);
  digitalWrite(5, HIGH);
}
